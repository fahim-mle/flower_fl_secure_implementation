# DockerSpawner image (change if you want a smaller base image)
DOCKER_JUPYTER_IMAGE=jupyter/datascience-notebook:latest
DOCKER_NETWORK_NAME=fl-internal-network

# Ports
HUB_API_PORT=8101
HUB_INTERNAL_PORT=8081
