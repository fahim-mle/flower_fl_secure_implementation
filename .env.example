PUBLIC_PORT=8100

# Keycloak admin credentials (dev only)
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_PORT=8080

# OAuth client secret used in fl-realm-realm.json (ranom hex string for example purposes)
OAUTH_CLIENT_SECRET=sdde3f4567g89h012i34jklmno567pqr
OAUTH_CLIENT_ID=jupyterhub-client

# JupyterHub cookie secret (optional override)
